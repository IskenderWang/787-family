<?xml version="1.0"?>
<PropertyList>

 <path>red-navlight.ac</path>
 <nopreview/>



 <effect>
   <inherits-from>Aircraft/787-family/Models/Lights/ALS/red-navlight</inherits-from>
   <object-name>red-navlight</object-name>
 </effect>
 
 <animation>
  <type>scale</type>
  <x-offset>0.5</x-offset>
  <y-offset>0.5</y-offset>
  <z-offset>0.5</z-offset>
 </animation>
 
<animation>
  <type>dist-scale</type>
  <object-name>red-navlight</object-name>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>1</dep>
   </entry>
   <entry>
    <ind>300</ind>
    <dep>4</dep>
   </entry>
   <entry>
    <ind>1500</ind>
    <dep>8</dep>
   </entry>
  </interpolation>
 </animation>


</PropertyList>
