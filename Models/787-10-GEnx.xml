        </center>
    </animation>

    <!-- Windshield Wipers :P -->

    <animation>
        <type>rotate</type>
        <object-name>wiper.l</object-name>
        <property>controls/wipers/degrees</property>
        <factor>1.4</factor>
        <axis>
            <x1-m>-24.5469</x1-m>
            <y1-m>-0.1953</y1-m>
            <z1-m>1.7102</z1-m>
            <x2-m>-24.5403</x2-m>
            <y2-m>-0.1917</y2-m>
            <z2-m>1.7049</z2-m>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>wiper.r</object-name>
        <property>controls/wipers/degrees</property>
        <factor>-1.4</factor>
        <axis>
            <x1-m>-24.5469</x1-m>
            <y1-m>0.1953</y1-m>
            <z1-m>1.7102</z1-m>
            <x2-m>-24.5403</x2-m>
            <y2-m>0.1917</y2-m>
            <z2-m>1.7049</z2-m>
        </axis>
    </animation>

    <!-- Ground stuff -->

    <model>
        <name>ground-lights</name>
        <path>Ground/landing-ground-lights.xml</path>
    </model>

    <animation>
        <type>select</type>
        <object-name>ground-lights</object-name>
        <condition>
            <property>controls/lighting/landing-lights[0]</property>
            <less-than>
                <property>position/altitude-agl-ft</property>
                <value>500</value>
            </less-than>
        </condition>
    </animation>

    <model>
        <name>taxi-lights</name>
        <path>Ground/taxi-ground-lights.xml</path>
    </model>

    <animation>
        <type>select</type>
        <object-name>taxi-lights</object-name>
        <condition>
            <property>controls/lighting/taxi-lights</property>
            <less-than>
                <property>position/altitude-agl-ft</property>
                <value>500</value>
            </less-than>
            <equals>
                <property>gear/gear[0]/position-norm</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <!-- Window Lights -->

    <animation>
        <type>select</type>
        <object-name>windows-bright</object-name>
        <condition>
            <and>
                <greater-than-equals>
                    <property>sim/time/sun-angle-rad</property>
                    <value>1.57</value>
                </greater-than-equals>
                <or>
                    <greater-than-equals>
                        <property>/position/altitude-agl-ft</property>
                        <value>8000</value>
                    </greater-than-equals>
                    <property>controls/electric/external-power</property>
                </or>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>windows-dim</object-name>
        <condition>
            <and>
                <greater-than-equals>
                    <property>sim/time/sun-angle-rad</property>
                    <value>1.57</value>
                </greater-than-equals>
                <less-than>
                    <property>/position/altitude-agl-ft</property>
                    <value>8000</value>
                </less-than>
                <not>
                    <property>controls/electric/external-power</property>
                </not>
            </and>
        </condition>
    </animation>

    <!-- Fuel Truck -->

    <model>
        <path>Aircraft/787-family/Models/Ground/Services/Fuel-Truck/Fuel_Truck_Short_VolvoFM.ac</path>
        <condition>
            <equals>
                <property>services/fuel-truck/enable</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

    <animation>
        <type>select</type>
        <object-name>fuel_pipe</object-name>
        <condition>
            <equals>
                <property>services/fuel-truck/connect</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>fuel_pipe_disconnected</object-name>
        <condition>
            <equals>
                <property>services/fuel-truck/connect</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>

    <!-- De-icing Truck -->

    <model>
        <path>Aircraft/787-family/Models/Ground/Services/De-Icing/volvo_deicing_truck.xml</path>
        <condition>
            <equals>
                <property>services/deicing_truck/enable</property>
                <value>1</value>
            </equals>
        </condition>
        <offsets>
            <x-m>-17</x-m>
            <y-m>9.5</y-m>
            <z-m>-3.77232</z-m>
            <heading-deg>160</heading-deg>
        </offsets>
    </model>

    <!-- Catering Truck -->

    <model>
        <path>Aircraft/787-family/Models/Ground/Services/Catering/Catering_Loader_Truck_VolvoFM.xml</path>
        <condition>
            <equals>
                <property>services/catering/enable</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

    <!-- Baggage Truck -->

    <model>
        <path>Aircraft/787-family/Models/Ground/Services/Baggage/cargo_ramp.ac</path>
        <condition>
            <equals>
                <property>services/baggage/enable</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

    <animation>
        <type>select</type>
        <object-name>cargo_door</object-name>
        <object-name>cargo_inside</object-name>
        <condition>
            <equals>
                <property>services/baggage/enable</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <!-- Stairs -->

    <model>
    <path>Aircraft/787-family/Models/Ground/Services/Stairs/stairs.xml</path>
    <condition>
        <equals>
        <property>services/stairs/enable</property>
        <value>1</value>
        </equals>
    </condition>
    <offsets>
        <x-m>-27.823</x-m> <!-- front to back -->
        <y-m>-9.8276</y-m> <!-- side to side -->
        <z-m>1</z-m> <!-- height -->
    </offsets>
    </model>

    <!-- External Power -->

    <model>
        <path>Aircraft/787-family/Models/Ground/Services/External-Power/ext-pwr-box.ac</path>
        <condition>
            <equals>
                <property>services/ext-pwr/enable</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

    <!-- Chocks -->

    <model>
        <name>chock_nose</name>
        <path>Aircraft/787-family/Models/Ground/Services/Chocks-10/chock_nose.ac</path>
        <condition>
            <equals>
                <property>services/chocks/nose</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

    <model>
        <name>chock_left</name>
        <path>Aircraft/787-family/Models/Ground/Services/Chocks-9/chock_left.ac</path>
        <condition>
            <equals>
                <property>services/chocks/left</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

    <model>
        <name>chock_right</name>
        <path>Aircraft/787-family/Models/Ground/Services/Chocks-9/chock_right.ac</path>
        <condition>
            <equals>
                <property>services/chocks/right</property>
                <value>1</value>
            </equals>
        </condition>
    </model>

</PropertyList>
