<?xml version="1.0" encoding="UTF-8"?>

<PropertyList include="787-common.xml">

    <sim n="0">
        <description>Boeing 787-9 (Trent 1000)</description>
        <flight-model>jsb</flight-model>
        <aero>787-9-Trent</aero>
        <variant-of>787-9-GEnx</variant-of>
        <authors>
          See Authors.md for details.
        </authors>
        <aircraft-version>3.6</aircraft-version>
        <rating>
            <FDM type="int">4</FDM>
            <systems type="int">4</systems>
            <model type="int">4</model>
            <cockpit type="int">4</cockpit>
        </rating>
        <tags>
            <tag>transport</tag>
            <tag>boeing</tag>
            <tag>2010s</tag> <!-- first flight 2009, entry into service 2011 -->
            <tag>etops</tag>
            <tag>glass-cockpit</tag>
            <tag>hud</tag>
            <tag>ifr</tag>
            <tag>retractable-gear</tag>
            <tag>tricycle</tag>
            <tag>jet</tag>
            <tag>twin-engine</tag>
        </tags>
        <engine>trent_1000-9</engine>

        <model>
            <path>Aircraft/787-family/Models/787-9-Trent1000.xml</path>

            <livery>
                <file type="string">BOE-9</file>
            </livery>

            <b787>
                <heading-bug-pfd-deg type="double">0</heading-bug-pfd-deg>
                <heading-bug-deg type="double">0</heading-bug-deg>
                <nav1-bug-deg type="double">0</nav1-bug-deg>
                <nav2-bug-deg type="double">0</nav2-bug-deg>
                <wp-bug-deg type="double">0</wp-bug-deg>
                <ias-bug-kt-norm type="double">0</ias-bug-kt-norm>
                <mach-bug-kt-norm type="double">0</mach-bug-kt-norm>
            </b787>
            <door>
                <position-x-m type="float">-23.6</position-x-m>
                <position-y-m type="float">-2.4</position-y-m>
                <position-z-m type="float">1</position-z-m>
                <jetway-hood-deg type="float">0</jetway-hood-deg>
            </door>

            <pushback>
                <kp type="double">300</kp>
                <ki type="double">25</ki>
                <kd type="double">0</kd>
                <position-norm type="double">0</position-norm>
            </pushback>

            <efb>
                <diagram>help.jpg</diagram>
                <chart>help.jpg</chart>
            </efb>
            <shadow-enabled type="bool">true</shadow-enabled> <!-- disabled in remote aircraft over mp -->
        </model>

        <hud>
            <path n="1">Aircraft/787-family/Models/FlightDeck/HUD2.xml</path>
        </hud>

        <flaps>
            <setting>0.000</setting> <!-- flaps up -->
            <setting>0.033</setting> <!-- flaps 1  -->
            <setting>0.220</setting> <!-- flaps 5  -->
            <setting>0.360</setting> <!-- flaps 10  -->
            <setting>0.500</setting> <!-- flaps 15 -->
            <setting>0.567</setting> <!-- flaps 17 -->
            <setting>0.600</setting> <!-- flaps 18 -->
            <setting>0.667</setting> <!-- flaps 20 -->
            <setting>0.833</setting> <!-- flaps 25 -->
            <setting>1.000</setting> <!-- flaps 30 -->
        </flaps>

        <startup>
            <splash-texture>Aircraft/787-family/splash1.png</splash-texture>
        </startup>

        <!-- Initialize External Connections as False, the clients will change them to true if connection is established -->

        <connection>
            <fgfscopilot>
                <connected type="bool">false</connected>
            </fgfscopilot>
            <autopirep>
                <connected type="bool">false</connected>
            </autopirep>
        </connection>

        <chase-distance-m>-100</chase-distance-m>

        <sound>
            <path>Aircraft/787-family/b787-sound.xml</path>
        </sound>

        <instrumentation>
            <path>Aircraft/787-family/Systems/instrumentation.xml</path>

            <tcas>
                <!-- http://wiki.flightgear.org/index.php/TCAS -->

                <serviceable type="bool">true</serviceable>
                <!-- TCAS ENABLE -->

                <inputs>
                    <mode type="int">3</mode> <!-- 0=off, 1=standby, 2=TA-only, 3=auto(TA/RA) -->
                </inputs>

                <voice>
                    <file-prefix type="string">Aircraft/MyAircraft/Sounds/tcas/</file-prefix>
                </voice>

                <speaker>
                    <!-- Speaker Configuration -->
                    <max-dist> 12 </max-dist> <!-- Max. distance where speaker is heard -->
                    <reference-dist> 2 </reference-dist> <!-- Distance to pilot -->
                    <volume> 2.0 </volume> <!-- Volume at reference distance -->
                </speaker>

            </tcas>

            <sysinfo>
                <max type="int">0</max>
                <first type="int">0</first>
            </sysinfo>

            <wxradar>
                <serviceable type="bool">true</serviceable>
                <multi-scan type="bool">true</multi-scan>
                <tilt type="int">0</tilt>
                <precip type="bool">true</precip>
                <range type="int">30</range>
            </wxradar>

            <warn>
                <master-sound type="int">1</master-sound>
            </warn>

            <nd-settings>
                <wpt type="bool">true</wpt>
                <arpt type="bool">true</arpt>
                <vor type="bool">true</vor>
                <ndb type="bool">true</ndb>
                <traffic type="bool">false</traffic>
            </nd-settings>

            <nd n="0">
                <radar-texture-path type="string">Aircraft/787-family/Models/Instruments/od_ndradar_1.png</radar-texture-path>
                <symbol-texture-path type="string">Aircraft/787-family/Models/Instruments/symbols_2d.png</symbol-texture-path>
                <font>
                    <color>
                        <red type="float">0.7</red>
                        <green type="float">0.7</green>
                        <blue type="float">0.7</blue>
                        <alpha type="float">1</alpha>
                    </color>
                    <name type="string">helvetica_bold.txf</name>
                    <size type="float">14</size>
                    <line-spacing type="float">0.01</line-spacing>
                </font>
                <serviceable type="bool">true</serviceable>
                <range type="int">30</range>
                <aircraft-heading-up type="bool">true</aircraft-heading-up>
                <heading-up-deg type="float">0.01</heading-up-deg>
                <x-center type="float">0.499</x-center>
                <y-center type="float">0.182</y-center>
            </nd>

            <nd n="1">
                <radar-texture-path type="string">Aircraft/787-family/Models/Instruments/od_ndradar_2.png</radar-texture-path>
                <symbol-texture-path type="string">Aircraft/787-family/Models/Instruments/symbols_2d.png</symbol-texture-path>
                <font>
                    <color>
                        <red type="float">0.7</red>
                        <green type="float">0.7</green>
                        <blue type="float">0.7</blue>
                        <alpha type="float">1</alpha>
                    </color>
                    <name type="string">helvetica_bold.txf</name>
                    <size type="float">14</size>
                    <line-spacing type="float">0.01</line-spacing>
                </font>
                <serviceable type="bool">true</serviceable>
                <range type="int">100</range>
                <aircraft-heading-up type="bool">true</aircraft-heading-up>
                <heading-up-deg type="float">0.01</heading-up-deg>
                <x-center type="float">0.499</x-center>
                <y-center type="float">0.182</y-center>
            </nd>

        </instrumentation>

        <alarms>
            <master-caution type="bool">false</master-caution>
        </alarms>

        <!-- The cockpit is -3.25 z-offset from the cockpit of the 787-8 -->
        <view n="0">
            <name>Pilot View</name>
            <type>lookfrom</type>
            <internal type="bool">true</internal>
            <cockpit type="bool">true</cockpit>
            <config>
                <x-offset-m type="double">-0.568</x-offset-m>
                <y-offset-m type="double">2.298</y-offset-m>
                <z-offset-m type="double">-25.86</z-offset-m>
                <pitch-offset-deg>-10</pitch-offset-deg>
                <default-field-of-view-deg type="double">90.0</default-field-of-view-deg>
            </config>
        </view>

        <view n="1">
            <internal type="bool">0</internal>
            <cockpit type="bool">0</cockpit>
        </view>

        <view n="2">
            <internal type="bool">0</internal>
            <cockpit type="bool">0</cockpit>
        </view>

        <view n="3">
            <internal type="bool">0</internal>
            <cockpit type="bool">0</cockpit>
        </view>

        <view n="4">
            <internal type="bool">0</internal>
            <cockpit type="bool">0</cockpit>
        </view>

        <view n="5">
            <internal type="bool">0</internal>
            <cockpit type="bool">0</cockpit>
        </view>

