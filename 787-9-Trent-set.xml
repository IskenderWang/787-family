        <menubar>
            <default>
                <menu n="3">
                    <item n="0">
                        <label>Autopilot Dialog</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var ap_dialog = gui.Dialog.new("sim/gui/dialogs/ap_dialog/dialog", "Aircraft/787-family/gui/dialogs/it-autoflight-dlg.xml");
                             ap_dialog.open();
                            </script>
                        </binding>
                    </item>
                </menu>
                <menu n="5">
                    <item n="5">
                        <label>Radio Stack</label>
                        <binding>
                            <command>nasal</command>
                            <script>dialogs.Radio.open()</script>
                        </binding>
                    </item>
                </menu>
                <menu n="100">
                    <label>Boeing 787-9</label>
                    <enabled type="bool">true</enabled>
                    <item>
                        <name>autostart</name>
                        <label>Autostart Aircraft Engines</label>
                        <binding>
                            <command>property-toggle</command>
                            <property>sim/model/start-idling</property>
                        </binding>
                    </item>
                    <item>
                        <label>Select Aircraft Livery</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             aircraft.livery.dialog.open();
                            </script>
                        </binding>
                    </item>

                    <item>
                        <label>Virtual Co-pilot Settings</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var copilotdialog = gui.Dialog.new("sim/gui/dialogs/copilot/dialog", "Aircraft/787-family/Systems/copilot-dlg.xml");
                             copilotdialog.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Import/Export Flightplan</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var flightplan = gui.Dialog.new("sim/gui/dialogs/flightplan/dialog", "Aircraft/787-family/Systems/flightplan-dlg.xml");
                             flightplan.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <name>autopush</name>
                        <label>Pushback</label>
                        <binding>
                            <command>dialog-show</command>
                            <dialog-name>autopush</dialog-name>
                        </binding>
                    </item>
                    <item>
                        <name>tiller</name>
                        <label>Tiller Steering</label>
                        <binding>
                            <command>dialog-show</command>
                            <dialog-name>tiller-dlg</dialog-name>
                        </binding>
                    </item>
                    <item>
                        <name>lights</name>
                        <label>Control Aircraft Lights</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var lights = gui.Dialog.new("sim/gui/dialogs/lights/dialog", "Aircraft/787-family/Systems/lights-dlg.xml");
                             lights.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <name>announcements</name>
                        <label>Make Announcements</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var announce = gui.Dialog.new("sim/gui/dialogs/announcements/dialog", "Aircraft/787-family/Systems/announcements-dlg.xml");
                             announce.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Simulate System Failures</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var failures = gui.Dialog.new("sim/gui/dialogs/failures/dialog", "Aircraft/787-family/Systems/failures-dlg.xml");
                             failures.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Ground Services' Controls</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                            var ground_services = gui.Dialog.new("sim/gui/dialogs/ground_services/dialog", "Aircraft/787-family/Systems/ground_services-dlg.xml");
                             ground_services.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Fuel Truck Controls</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                            var fuel_truck = gui.Dialog.new("sim/gui/dialogs/fuel_truck/dialog", "Aircraft/787-family/Systems/fuel_truck.xml");
                             fuel_truck.open();
                            </script>
                        </binding>
                    </item>
                    <!-- item>
                        <name>dualcontrol</name>
                        <label>Dual Control System Settings</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                             var dualcontrol = gui.Dialog.new("sim/gui/dialogs/dualcontrol/dialog", "Aircraft/787-family/Systems/dualcontrol-dlg.xml");
                             dualcontrol.open();
                            </script>
                        </binding>
                    </item -->
                </menu>
            </default>
        </menubar>


        <help>
            <title>Boeing 787-9</title>
            <key>
                <name>Delete</name>
                <desc>Toggle thrust reversers</desc>
            </key>
            <key>
                <name>e</name>
                <desc>Set idle thrust</desc>
            </key>
            <key>
                <name>r</name>
                <desc>Set TOGA thrust</desc>
            </key>
            <key>
                <name>Ctrl-f</name>
                <desc>Toggle FBW override</desc>
            </key>
            <key>
                <name>Ctrl-y</name>
                <desc>Toggle yaw damper</desc>
            </key>
            <key>
                <name>Ctrl-b</name>
                <desc>Cycle speedbrake lever setting</desc>
            </key>
            <key>
                <name>Ctrl-k</name>
                <desc>Arm/disarm ground spoilers</desc>
            </key>
            <key>
                <name>w</name>
                <desc>Walk forward toward view direction</desc>
            </key>
            <key>
                <name>W</name>
                <desc>Run forward toward view direction</desc>
            </key>
            <key>
                <name>s</name>
                <desc>Walk backward from view direction</desc>
            </key>
        </help>

        <!-- General MP Enabled Properties -->

        <multiplay>
            <generic>

                <!-- Aircraft Lighting -->

                <int n="0" alias="controls/lighting/logo" />
                <int n="1" alias="controls/lighting/landing-lights[0]" />
                <int n="2" alias="controls/lighting/wing-lights" />
                <int n="3" alias="sim/model/lights/beacon/state" />
                <int n="4" alias="controls/lighting/nav-lights" />
                <int n="5" alias="sim/model/lights/strobe/state" />
                <int n="6" alias="controls/lighting/taxi-lights" />

                <!-- Gears, Engines, Surfaces and Wingflex -->

                <float n="0" alias="gear/gear[0]/position-norm" />
                <float n="1" alias="gear/gear[1]/position-norm" />
                <float n="2" alias="gear/gear[2]/position-norm" />
                <float n="3" alias="engines/engine[0]/n1" />
                <float n="4" alias="engines/engine[1]/n1" />
                <float n="5" alias="sim/systems/wingflexer/z-m" />
                <float n="6" alias="/fdm/jsbsim/fcs/aileron-cmd-norm" />
                <float n="7" alias="surface-positions/flap-pos-norm" />
