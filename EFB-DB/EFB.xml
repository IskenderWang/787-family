<?xml version="1.0"?>

<PropertyList>

	<path>efb.ac</path>

<!-- Emission -->

 	<animation>
    	<type>material</type>
		<object-name>efis</object-name>
    	<condition>
        	<property>controls/lighting/cockpit</property>
  		</condition>
    	<emission>
      		<red>   0.18 </red>
      		<green> 0.18 </green>
      		<blue>  0.18 </blue>
    	</emission>
  	</animation>

<!-- Come on only when there's atleast 9 volts of power -->

	<animation>
		<type>select</type>
		<object-name>text</object-name>
		<object-name>worldmap</object-name>
		<object-name>airportmarker</object-name>
		<object-name>worldmap2</object-name>
		<object-name>marker</object-name>
		<object-name>diagram</object-name>
		<object-name>chart</object-name>
		<object-name>diagramrot1</object-name>
		<object-name>chartrot1</object-name>
		<object-name>diagramrot2</object-name>
		<object-name>chartrot2</object-name>
		<object-name>checklists</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/efis</property>
				<value>9</value>
			</greater-than>
		</condition>
	</animation>

<!-- Page Specific Objects -->

	<animation>
		<type>select</type>
		<object-name>worldmap</object-name>
		<object-name>airportmarker</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/page</property>
				<value>Airport Information</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>worldmap2</object-name>
		<object-name>marker</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/page</property>
				<value>GPS Settings</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>diagram</object-name>
		<object-name>diagramrot1</object-name>
		<object-name>diagramrot2</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/page</property>
				<value>Airport Diagram</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>chart</object-name>
		<object-name>chartrot1</object-name>
		<object-name>chartrot2</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/page</property>
				<value>Airport Charts</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>checklists</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/checklists/show</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>opsmanual</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/page</property>
				<value>Operating Manual</value>
			</equals>
		</condition>
	</animation>

<!-- Chart and Diagram Rotations -->

	<animation>
		<type>select</type>
		<object-name>diagram</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/diagram/rotation</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>diagramrot1</object-name>
		<object-name>diagramrot2</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/diagram/rotation</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>chart</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/chart/rotation</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>chartrot1</object-name>
		<object-name>chartrot2</object-name>
		<condition>
			<equals>
				<property>/instrumentation/efb/chart/rotation</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>


