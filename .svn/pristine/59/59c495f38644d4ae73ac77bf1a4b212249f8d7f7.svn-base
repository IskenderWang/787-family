<?xml version="1.0"?>

<PropertyList>

	<path>pfd.ac</path>

<!-- Come on only when there's atleast 9 volts of power -->

	<animation>
		<type>select</type>
		<object-name>pfd</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/efis</property>
				<value>9</value>
			</greater-than>
		</condition>
	</animation>

<!-- Altitude Tape -->

<!-- ALT tape-->

    <animation>
        <type>select</type>
        <object-name>Alt1</object-name>
        <condition>
            <less-than>
                <property>instrumentation/altimeter/indicated-altitude-ft</property>
                <value>9600</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Alt2</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>9599.999</value>
                </greater-than>
                <less-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>19200</value>
                </less-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Alt3</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>19199.999</value>
                </greater-than>
                <less-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>28800</value>
                </less-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Alt4</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>28799.999</value>
                </greater-than>
                <less-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>38400</value>
                </less-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Alt5</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>38399.999</value>
                </greater-than>
                <less-than>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <value>48000</value>
                </less-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Alt6</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/altimeter/indicated-altitude-ft</property>
                <value>47999.999</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Alt1</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <interpolation>
            <entry><ind>-200</ind><dep>-0.0187</dep></entry>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>9600</ind><dep>0.9035</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Alt2</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <interpolation>
            <entry><ind>9600</ind><dep>0</dep></entry>
            <entry><ind>19200</ind><dep>0.9035</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Alt3</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <interpolation>
            <entry><ind>19200</ind><dep>0</dep></entry>
            <entry><ind>28800</ind><dep>0.9035</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Alt4</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <interpolation>
            <entry><ind>28800</ind><dep>0</dep></entry>
            <entry><ind>38400</ind><dep>0.9035</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Alt5</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <interpolation>
            <entry><ind>38400</ind><dep>0</dep></entry>
            <entry><ind>48000</ind><dep>0.9035</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Alt6</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <interpolation>
            <entry><ind>48000</ind><dep>0</dep></entry>
            <entry><ind>57600</ind><dep>0.9035</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

<!-- Speed Tape -->

    <animation>
        <type>textranslate</type>
        <object-name>ASI</object-name>
        <property>velocities/airspeed-kt</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>560</ind><dep>0.7520</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>

<!-- Glideslope pointer -->

    <animation>
        <type>select</type>
        <object-name>gsscale</object-name>
        <condition>
            <property>instrumentation/nav/has-gs</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>gspointer</object-name>
        <condition>
            <and>
                <property>instrumentation/nav/gs-in-range</property>
                <property>instrumentation/nav/has-gs</property>
            </and>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>gspointer</object-name>
        <property>instrumentation/nav/gs-needle-deflection-norm</property>
        <factor>0.0272</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

<!-- LOC pointer -->

    <animation>
        <type>select</type>
        <object-name>locscale</object-name>
        <condition>
            <property>instrumentation/nav/nav-loc</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>locpointer</object-name>
        <condition>
            <and>
                <property>instrumentation/nav/in-range</property>
                <property>instrumentation/nav/nav-loc</property>
            </and>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>locpointer</object-name>
        <property>instrumentation/nav/heading-needle-deflection-norm</property>
        <factor>-0.0272</factor>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

<!-- HSI -->

    <animation>
        <type>texrotate</type>
        <object-name>Hsi</object-name>
        <property>orientation/heading-magnetic-deg</property>
        <center>
            <x>0.75</x>
            <y>0.75</y>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>

<!-- Clock -->

    <animation>
        <type>rotate</type>
        <object-name>Chrono.needle</object-name>
        <property>instrumentation/clock/indicated-sec</property>
        <factor>6</factor>
        <center>
            <x-m>0.0</x-m>
            <y-m>-0.1360</y-m>
            <z-m>0.1225</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
<!-- Digital displays -->

    <animation>
        <type>textranslate</type>
        <object-name>Hr.10</object-name>
        <property>instrumentation/clock/indicated-hour</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Hr.01</object-name>
        <property>instrumentation/clock/indicated-hour</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
 
    <animation>
        <type>textranslate</type>
        <object-name>Mn.10</object-name>
        <property>instrumentation/clock/indicated-min</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Mn.01</object-name>
        <property>instrumentation/clock/indicated-min</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ET10</object-name>
        <property>instrumentation/clock/ET-display</property>
        <factor>0.0001</factor>
        <step>1000</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ET01</object-name>
        <property>instrumentation/clock/ET-display</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ET.10</object-name>
        <property>instrumentation/clock/ET-display</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ET.01</object-name>
        <property>instrumentation/clock/ET-display</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ET10</object-name>
        <object-name>ET01</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/clock/ET-display</property>
                <value>99.99</value>
            </greater-than>
        </condition>
    </animation>

<!-- Horizon -->

    <animation>
        <type>texmultiple</type>
        <object-name>horizon</object-name>
        <transform>
            <property>orientation/pitch-deg</property>
            <subtype>textranslate</subtype>
            <factor>0.00236882</factor>
            <axis>
                <x>0</x>
                <y>1</y>
            </axis>
        </transform>
        <transform>
            <property>orientation/roll-deg</property>
            <subtype>texrotate</subtype>
            <center>
                <x>0.50</x>
                <y>0.50</y>
            </center>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>-1</z>
            </axis>
        </transform>
    </animation>

<!-- Warnings -->

<animation>
    <type>select</type>
	<object-name>warn2</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/ice/wing/temp</property>
    		<value>0</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn3</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/ice/eng1/temp</property>
    		<value>0</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn4</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/ice/eng2/temp</property>
    		<value>0</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn.4</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/ice/wing/temp</property>
    		<value>-20</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn5</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/ice/eng1/temp</property>
    		<value>-20</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn6</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/ice/eng2/temp</property>
    		<value>-20</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn1</object-name>
  	<condition>
	<less-than-equals>
 	   	<property>controls/window-heat/temp</property>
    		<value>0</value>
	</less-than-equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn7</object-name>
  	<condition>
	<not>
    	<equals>
    	<property>surface-positions/speedbrake-pos-norm</property>
    	<value>0</value>
    	</equals>
	</not>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>warn9</object-name>
  	<condition>
	<equals>
 	   	<property>controls/hydraulic/failure-indicator</property>
    		<value>1</value>
	</equals>
  	</condition>
</animation>

<!-- Flaps Indicator -->

<animation>
   	<type>textranslate</type>
   	<object-name>flaps</object-name>
   	<property>surface-positions/flap-pos-norm</property>
   	<factor>0.36</factor>
     <axis>
     	<x>0.0</x>
     	<y>1.0</y>
     	<z>0.0</z>
   	</axis>
</animation>

<!-- Gears Indicator -->

<animation>
    <type>select</type>
	<object-name>ngear</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[0]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>lgear</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[1]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>rgear</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[2]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

<!-- Flight Director -->

<animation>
    <type>select</type>
	<object-name>flt.dir</object-name>
	<object-name>FD.roll</object-name>
	<object-name>FD.pitch</object-name>
  	<condition>
    	<property>autopilot/flightdirector</property>
  	</condition>
</animation>

    <animation>
        <type>translate</type>
        <object-name>FD.roll</object-name>
        <property>autopilot/internal/target-roll-deg</property>
        <factor>0.0020222222</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>FD.pitch</object-name>
        <property>autopilot/internal/climb-rate-difference</property>
        <factor>0.0010222222</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

</PropertyList>
