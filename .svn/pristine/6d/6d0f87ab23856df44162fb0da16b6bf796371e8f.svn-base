<?xml version="1.0"?>

<PropertyList>

	<path>hyd.ac</path>

<!-- Come on only when there's atleast 9 volts of power -->

	<animation>
		<type>select</type>
		<object-name>hyd</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/efis</property>
				<value>9</value>
			</greater-than>
		</condition>
	</animation>

<!-- Warn lights Select -->

<animation>
	<type>select</type>
	<object-name>e1.warn</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys0-eng</property>
				<value>70</value>
			</greater-than-equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>e2.warn</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys1-eng</property>
				<value>70</value>
			</greater-than-equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>c1.warn</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys2-elec</property>
				<value>70</value>
			</greater-than-equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>c2.warn</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys3-elec</property>
				<value>70</value>
			</greater-than-equals>
	</condition>
</animation>

<!-- Fault Warnings -->

<animation>
	<type>select</type>
	<object-name>e1.fail</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys0-eng</property>
				<value>120</value>
			</greater-than-equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>e2.fail</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys1-eng</property>
				<value>120</value>
			</greater-than-equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>c1.fail</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys2-elec</property>
				<value>120</value>
			</greater-than-equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>c2.fail</object-name>
	<condition>
			<greater-than-equals>
				<property>controls/pneumatic/temp/hyd-sys3-elec</property>
				<value>120</value>
			</greater-than-equals>
	</condition>
</animation>

<!-- Run lights -->

<animation>
	<type>select</type>
	<object-name>e1.run</object-name>
	<condition>
			<equals>
				<property>controls/hydraulic/system/engine-pump</property>
				<value>1</value>
			</equals>
	</condition>
</animation>
<animation>
	<type>select</type>
	<object-name>e2.run</object-name>
	<condition>
			<equals>
				<property>controls/hydraulic/system[1]/engine-pump</property>
				<value>1</value>
			</equals>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>e.run</object-name>
	<condition>
			<equals>
				<property>controls/hydraulic/systeme</property>
				<value>1</value>
			</equals>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>c1.run</object-name>
	<condition>
			<equals>
				<property>controls/hydraulic/system[2]/electric-pump</property>
				<value>1</value>
			</equals>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>c2.run</object-name>
	<condition>
			<equals>
				<property>controls/hydraulic/system[3]/electric-pump</property>
				<value>1</value>
			</equals>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>c.run</object-name>
	<condition>
			<equals>
				<property>controls/hydraulic/systemc</property>
				<value>1</value>
			</equals>
	</condition>
</animation>

<!-- Power Indicators -->

<animation>
	<type>select</type>
	<object-name>e1.pwr</object-name>
	<condition>
			<greater-than-equals>
				<property>engines/engine/n1</property>
				<value>20</value>
			</greater-than-equals>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>e2.pwr</object-name>
	<condition>
			<greater-than-equals>
				<property>engines/engine[1]/n1</property>
				<value>20</value>
			</greater-than-equals>
	</condition>
</animation>

<!-- Pick Animation -->

 <animation>
   <type>pick</type>
   <object-name>e1.click</object-name>
	<action>
        <button>0</button>
	<repeatable type="bool">false</repeatable>
	<binding>
		<command>property-cycle</command>
		<property>controls/hydraulic/system[0]/setting</property>
		<value>0</value>
		<value>2</value>
	</binding>
	</action>
 </animation>

 <animation>
   <type>pick</type>
   <object-name>e2.click</object-name>
	<action>
        <button>0</button>
	<repeatable type="bool">false</repeatable>
	<binding>
		<command>property-cycle</command>
		<property>controls/hydraulic/system[1]/setting</property>
		<value>0</value>
		<value>2</value>
	</binding>
	</action>
 </animation>

 <animation>
   <type>pick</type>
   <object-name>c1.click</object-name>
	<action>
        <button>0</button>
	<repeatable type="bool">false</repeatable>
	<binding>
		<command>property-cycle</command>
		<property>controls/hydraulic/system[2]/setting</property>
		<value>0</value>
		<value>2</value>
	</binding>
	</action>
 </animation>

 <animation>
   <type>pick</type>
   <object-name>c2.click</object-name>
	<action>
        <button>0</button>
	<repeatable type="bool">false</repeatable>
	<binding>
		<command>property-cycle</command>
		<property>controls/hydraulic/system[3]/setting</property>
		<value>0</value>
		<value>2</value>
	</binding>
	</action>
 </animation>

</PropertyList>
