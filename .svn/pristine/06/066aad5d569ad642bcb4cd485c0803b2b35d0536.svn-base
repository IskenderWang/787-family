<?xml version="1.0"?>

<PropertyList>

	<path>mfd-nav.ac</path>

	<!-- HSI arc -->
	<animation>
		<name>HSI arc</name>
		<type>texrotate</type>
		<object-name>arc</object-name>
		<property>orientation/heading-magnetic-deg</property>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
		<center>
			<x>0.5</x>
			<y>0.5</y>
		</center>
	</animation>

	<!-- VOR 1 -->
	<animation>
		<type>select</type>
		<object-name>vor1</object-name>
		<condition>
				<less-than-equals>
					<property>instrumentation/nav/nav-distance</property>
					<value>55600</value>
				</less-than-equals>
		</condition>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>vor1</object-name>
		<property>controls/navigation/nav1-defl</property>
		<factor>0.85</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.0466</z-m>
		</center>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>vor1</object-name>
		<property>instrumentation/nav/nav-distance</property>
		<factor>0.00000198</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>vor1</object-name>
		<property>controls/navigation/nav1-defl</property>
		<factor>-0.85</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.0466</z-m>
		</center>
	</animation>

	<!-- VOR 2 -->
	<animation>
		<type>select</type>
		<object-name>vor2</object-name>
		<condition>
				<less-than-equals>
					<property>instrumentation/nav[1]/nav-distance</property>
					<value>55600</value>
				</less-than-equals>
		</condition>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>vor2</object-name>
		<property>controls/navigation/nav2-defl</property>
		<factor>0.85</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.0466</z-m>
		</center>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>vor2</object-name>
		<property>instrumentation/nav[1]/nav-distance</property>
		<factor>0.00000198</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
