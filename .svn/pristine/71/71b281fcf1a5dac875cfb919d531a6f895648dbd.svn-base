	<path>Aircraft/787-8/Models/Ground/Services/Chokes/choke_left.ac</path>
	<condition>
		<equals>
			<property>services/chokes/left</property>
			<value>1</value>
		</equals>
	</condition>
</model>

<model>
	<name>choke_right</name>
	<path>Aircraft/787-8/Models/Ground/Services/Chokes/choke_right.ac</path>
	<condition>
