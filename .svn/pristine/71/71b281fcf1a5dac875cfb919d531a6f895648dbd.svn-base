	<!-- Lightmaps -->
	<effect>
		<inherits-from>Effects/lightmap</inherits-from>
		<parameters>
			<texture n="1">
				<image>Aircraft/787-8/Models/Effects/rudder_lightmap.png</image>
			</texture>
			<condition>
				<use>controls/lighting/logo</use>
			</condition>
		</parameters>
		<object-name>rudder</object-name>
		<object-name>fin</object-name>
	</effect>

	<effect>
		<inherits-from>Effects/lightmap</inherits-from>
		<parameters>
			<texture n="1">
				<image>Aircraft/787-8/Models/Effects/fuselage_lightmap.png</image>
			</texture>
			<condition>
				<use>controls/lighting/landing-lights[0]</use>
			</condition>
		</parameters>
		<object-name>fuselage</object-name>
		<object-name>bodyfairing</object-name>
	</effect>

	<effect>
		<inherits-from>Effects/lightmap</inherits-from>
		<parameters>
			<texture n="1">
				<image>Aircraft/787-8/Models/Effects/engines_lightmap.png</image>
			</texture>
			<condition>
				<use>controls/lighting/wing-lights</use>
			</condition>
		</parameters>
		<object-name>no2enge</object-name>
		<object-name>rhreverser</object-name>
		<object-name>rhvortexdev</object-name>
		<object-name>no1enge</object-name>
		<object-name>lhreverser</object-name>
		<object-name>lhvortexdev</object-name>
	</effect>

	<effect>
		<inherits-from>Effects/lightmap</inherits-from>
		<parameters>
			<texture n="1">
				<image>Aircraft/787-8/Models/Effects/wings_lightmap.png</image>
			</texture>
			<condition>
				<use>controls/lighting/wing-lights</use>
			</condition>
		</parameters>
		<object-name>wings_lightmap</object-name>
	</effect>

<animation>
  <type>select</type>
  <object-name>winglts</object-name>
  <condition>
    <equals>
      <property>controls/lighting/wing-lights</property>
      <value>1</value>
    </equals>
  </condition>
</animation>

