<!-- Livery Select -->

    <nasal>
        <load><![CDATA[
   var remote_aircraft_node = cmdarg ();
   remote_aircraft_node.getNode ("sim/model/shadow-enabled").setBoolValue (0);
   var livery_update = aircraft.livery_update.new("Aircraft/787-8/Models/Liveries");
        ]]></load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>

