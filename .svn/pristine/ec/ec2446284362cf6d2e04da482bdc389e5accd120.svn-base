		<keyboard>
 			<key n="267">
			<name>F11</name>
				<desc>Pop up autopilot dialog</desc>
				<binding>
							<command>nasal</command>
							<script>
							 var ap_dialog = gui.Dialog.new("sim/gui/dialogs/ap_dialog/dialog", "Aircraft/787-8/Systems/autopilot-dlg.xml");
							 ap_dialog.open();
							</script>
