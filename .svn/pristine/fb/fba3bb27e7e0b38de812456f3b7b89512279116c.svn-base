<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>../../Models/Lights/strobe.ac</path>

	<!-- ANIMATIONS -->
	<!-- On/off -->
	<animation>
		<name>On/off</name>
		<type>select</type>
		<object-name>Light</object-name>
		<condition>
			<property>sim/model/lights/strobe/state</property>
		</condition>
	</animation>

	<!-- Billboard animation -->
	<animation>
		<name>Billboard</name>
		<type>billboard</type>
		<object-name>Light</object-name>
	</animation>

</PropertyList>
