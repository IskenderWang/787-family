<?xml version="1.0"?>

<PropertyList>

	<path>mfd-menu.ac</path>

<!-- Come on only when there's atleast 9 volts of power -->

	<animation>
		<type>select</type>
		<object-name>menu</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/efis</property>
				<value>9</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>intro</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>intro</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>nav.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>nav</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>cdu.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>cdu</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>elec.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>elec</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>hyd.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>hyd</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>pneu.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>pneu</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>fuel.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>fuel</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>heat.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>heat</value>
  		</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>doors.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>doors</value>
  		</equals>
		</condition>
	</animation>

<model>
 <path>NAV.xml</path>
 <condition>
 <and>
  <equals>
   <property>controls/mfd3/page</property>
   <value>nav</value>
  </equals>
 </and>
 <and><not>
  <equals>
   <property>controls/mfd2/page</property>
   <value>nav</value>
  </equals>
 </not></and>
 </condition>
</model>

<model>
 <path>CDU1.xml</path>
 <offsets>
	<pitch-deg>90.0</pitch-deg>
 </offsets>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>cdu</value>
  </equals>
 </condition>
</model>

<model>
 <path>ELEC.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>elec</value>
  </equals>
 </condition>
</model>

<model>
 <path>HYD.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>hyd</value>
  </equals>
 </condition>
</model>

<model>
 <path>PNEU.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>pneu</value>
  </equals>
 </condition>
</model>

<model>
 <path>HEAT.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>heat</value>
  </equals>
 </condition>
</model>

<model>
 <path>FUEL.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>fuel</value>
  </equals>
 </condition>
</model>

<model>
 <path>DOORS.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>doors</value>
  </equals>
 </condition>
</model>

    <animation>
        <type>pick</type>
        <object-name>cdu</object-name>
        <object-name>cdu.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>cdu</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>elec</object-name>
        <object-name>elec.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>elec</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>hyd</object-name>
        <object-name>hyd.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>hyd</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>pneu</object-name>
        <object-name>pneu.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>pneu</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>fuel</object-name>
        <object-name>fuel.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>fuel</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>heat</object-name>
        <object-name>heat.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>heat</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>doors</object-name>
        <object-name>doors.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
		<value>doors</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>nav</object-name>
        <object-name>nav.active</object-name>
        <action>
            <button>0</button>
	    <repeatable type="bool">false</repeatable>
            <binding>
		<command>nasal</command>
 		<script>
			if (getprop("/controls/navigation/init") != 1) {
			b787.nav.init();
			b787.nd.init();
			}
			setprop("/controls/mfd3/page", "nav");
			setprop("/controls/navigation/init", 1); 
		</script>
            </binding>
        </action>
    </animation>
    
        <animation>
        <type>pick</type>
        <object-name>cam</object-name>
        <object-name>cam.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
				<value>cam</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>gear</object-name>
        <object-name>gear.active</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>controls/mfd3/page</property>
				<value>gear</value>
            </binding>
        </action>
    </animation>
    
    <animation>
		<type>select</type>
		<object-name>cam.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>cam</value>
  		</equals>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>gear.active</object-name>
		<condition>
 		<equals>
   			<property>controls/mfd3/page</property>
   			<value>gear</value>
  		</equals>
		</condition>
	</animation>
	
<model>
 <path>CAM.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>cam</value>
  </equals>
 </condition>
</model>

<model>
 <path>GEAR.xml</path>
 <condition>
  <equals>
   <property>controls/mfd3/page</property>
   <value>gear</value>
