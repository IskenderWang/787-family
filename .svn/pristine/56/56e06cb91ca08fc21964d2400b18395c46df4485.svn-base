<PropertyList>

<!-- Virtual Co-pilot Voices -->

    <copilot>
	<name>v1</name>
        <path>Aircraft/787-8/Sounds/v1.wav</path>
        <mode>once</mode>
        <condition>
            <property>sim/sound/v1</property>
        </condition>
        <volume>
            <factor>10</factor>
        </volume>
        <position>
            <x>0.00</x>
            <y>0.00</y>
            <z>0.00</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>100.0</max-dist>
    </copilot>

