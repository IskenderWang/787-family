<?xml version="1.0"?>

<PropertyList>

	<path>pneu.ac</path>

<!-- Come on only when there's atleast 9 volts of power -->

	<animation>
		<type>select</type>
		<object-name>pneu</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/efis</property>
				<value>9</value>
			</greater-than>
		</condition>
	</animation>

<!-- Light Text -->

	<animation>
		<name>White text</name>
		<type>material</type>
		<object-name>white</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>

<!-- Temperatures -->

	<text><name>white</name>
		<offsets>
			<x-m>0.0001</x-m>
			<y-m>-0.0204</y-m>
			<z-m>-0.1041</z-m>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type type="string">number-value</type>
		<format type="string">%3.0f°</format>
		<property>controls/pneumatic/temp/hyd-sys2-elec</property>
		<font>courier-bold.txf</font>
		<character-size>0.01</character-size>
		<character-aspect-ratio type="double">1.5</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
