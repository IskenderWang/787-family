<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <replay-config type="int">0</replay-config>
    <config n="0">

        <name>787 Flight Recorder</name>

        <!-- Generic tings -->

        <signals include="Aircraft/Generic/flightrecorder/components/position.xml"/>
        <signals include="Aircraft/Generic/flightrecorder/components/controls.xml"/>
        <signals include="Aircraft/Generic/flightrecorder/components/surfaces.xml"/>
        <signals include="Aircraft/Generic/flightrecorder/components/engine-jet.xml">
            <count type="int">2</count>
        </signals>
        <signals include="Aircraft/Generic/flightrecorder/components/gear-retractable.xml">
            <count type="int">3</count>
        </signals>
        <signals include="Aircraft/Generic/flightrecorder/components/tanks.xml">
            <count type="int">3</count>
        </signals>

        <!-- Add custom properties here -->

        <signal>
            <type>float</type>
            <property>/gear/gear/steering-norm</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/brake-thermal-energy</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/gear[0]/compression-norm</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/gear[1]/compression-norm</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/gear[2]/compression-norm</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/gear[0]/rollspeed-ms</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/gear[1]/rollspeed-ms</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/gear/gear[2]/rollspeed-ms</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/fdm/jsbsim/gear/unit[0]/wheel-speed-fps</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/fdm/jsbsim/gear/unit[1]/wheel-speed-fps</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/fdm/jsbsim/gear/unit[2]/wheel-speed-fps</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>bool</type>
            <property>/gear/gear[0]/tyre-smoke</property>
        </signal>
        <signal>
            <type>bool</type>
            <property>/gear/gear[1]/tyre-smoke</property>
        </signal>
        <signal>
            <type>bool</type>
            <property>/gear/gear[2]/tyre-smoke</property>
        </signal>
        <signal>
            <type>float</type>
            <property>/velocities/groundspeed-kt</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/position/gear-agl-ft</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/engines/engine[0]/reverser-pos-norm</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/engines/engine[1]/reverser-pos-norm</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/sim/systems/wingflexer/</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/sim/systems/wingflexer/params/</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/sim/systems/wingflexer/z-m</property>
            <interpolation>linear</interpolation>
        </signal>
        <signal>
            <type>float</type>
            <property>/systems/shake/shaking</property>
            <interpolation>linear</interpolation>
        </signal>

    </config>
</PropertyList>
