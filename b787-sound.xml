<!-- Engine External Sounds -->

    <engine>
        <name>left n1 external</name>
        <path>Aircraft/787-family/Sounds/whine.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <not>
                    <property>sim/current-view/internal</property>
                </not>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
        <pitch>
            <property>engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/n1</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.4</max>
        </volume>
        <volume>
            <property>sim/sound/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-8</y>
            <z>0</z>
        </position>
        <reference-dist>30.0</reference-dist>
        <max-dist>125.0</max-dist>
    </engine>   

