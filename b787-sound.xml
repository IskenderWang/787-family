<?xml version="1.0"?>

<PropertyList>
<params>
		<groundspeed-kt>velocities/groundspeed-kt</groundspeed-kt>
		<airspeed-kt>velocities/airspeed-kt</airspeed-kt>
</params>
<fx>

<!-- APU Sounds -->

    <apu-start>
        <name>apu-strt</name>
        <mode>once</mode>
        <path>Aircraft/787-family/Sounds/apu_strt.wav</path>
        <condition>
            <equals>
                <property>controls/APU/off-start-run</property>
                <value>1</value>
            </equals>
            <less-than>
                <property>engines/APU/rpm</property>
                <value>100</value>
            </less-than>
        </condition>
        <position>
            <x>26</x>
            <y>0</y>
            <z>4</z>
        </position>     
        <reference-dist>10</reference-dist> 
        <max-dist>30</max-dist> 
        <volume>0.3</volume>        
    </apu-start>

    <apu-running2>
        <name>apu-run2</name>
        <mode>looped</mode>
        <path>Aircraft/787-family/Sounds/apu_run.wav</path>
        <condition>
            <equals>
                <property>controls/APU/off-start-run</property>
                <value>1</value>
            </equals>
            <equals>
                <property>engines/APU/rpm</property>
                <value>100</value>
            </equals>
        </condition>
        <position>
            <x>26</x>
            <y>0</y>
            <z>4</z>
        </position> 
        <reference-dist>10</reference-dist>
        <max-dist>30</max-dist>
        <volume>0.3</volume>
    </apu-running2>
    
    
    <apu-running>
        <name>apu-run</name>
        <mode>looped</mode>
        <path>Aircraft/787-family/Sounds/apu_run.wav</path>
        <condition>
            <equals>
                <property>controls/APU/off-start-run</property>
                <value>2</value>
            </equals>
            <greater-than-equals>
                <property>engines/APU/rpm</property>
                <value>80</value>
            </greater-than-equals>
        </condition>
        <position>
            <x>26</x>
            <y>0</y>
            <z>4</z>
        </position> 
        <reference-dist>10</reference-dist>
        <max-dist>30</max-dist>
        <volume>0.3</volume>
    </apu-running>

    <apu-shutdown>
        <name>apu-stop</name>
        <mode>once</mode>
        <path>Aircraft/787-family/Sounds/apu_stop.wav</path>
        <condition>
            <equals>
                <property>controls/APU/off-start-run</property>
                <value>0</value>
            </equals>
            <less-than>
                <property>engines/APU/rpm</property>
                <value>100</value>
            </less-than>        
            <greater-than>
                <property>engines/APU/rpm</property>
                <value>0</value>
            </greater-than> 
        </condition>
        <position>
            <x>26</x>
            <y>0</y>
            <z>4</z>
        </position> 
        <reference-dist>10</reference-dist>
        <max-dist>30</max-dist>
        <volume>0.5</volume>
    </apu-shutdown> 

    <apu-shutdown2>
        <name>apu-stop2</name>
